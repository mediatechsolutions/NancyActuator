    <Version>0.1.0-alpha</Version>
    <PackageId>NancyActuator.PostgreSQLHealthCheck</PackageId>
    <Title>NancyActuator.PostgreSQLHealthCheck</Title>
    <Description>PostgreSQL connectivity health check for the NancyActuator library.</Description>
    <Authors>rubms;mediatechsolutions</Authors>
    <Copyright>Copyright Mediatech Solutions 2017</Copyright>
    <PackageLicenseUrl>https://www.gnu.org/licenses/lgpl-3.0.en.html</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/mediatechsolutions/NancyActuator</PackageProjectUrl>
    <PackageTags>Nancy NancyFx Actuator NancyActuator Postgre PostgreSQL Health Check</PackageTags>
    <IncludeSource>false</IncludeSource>
    <PackageReleaseNotes>First version.</PackageReleaseNotes>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Npgsql" Version="[3.2.2,4)" />
    <PackageReference Include="NancyActuator" Version="0.2.0-alpha" />
  </ItemGroup>
</Project>
